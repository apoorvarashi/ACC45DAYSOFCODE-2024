#include <iostream>
using namespace std;

int main() {
    // Declare an array of integers with size 5
    int numbers[5] = {10, 20, 30, 40, 50};

    // Access and print each element in the array
    cout << "Elements of the array are: " << endl;
    for (int i = 0; i < 5; i++) {
        cout << "Element at index " << i << " is: " << numbers[i] << endl;
    }

    // Modify elements in the array
    numbers[2] = 100;  // Changing the third element to 100
    cout << "\nAfter modifying the third element:" << endl;
    for (int i = 0; i < 5; i++) {
        cout << "Element at index " << i << " is: " << numbers[i] << endl;
    }

    return 0;
}
